// settingspage styles
#settingspage
    background-color rgba(0, 0, 0, 0.8)
    position fixed
    z-index: 99
    top 0
    lef 0
    height 100vh
    width 100%
    transform translateX(-102%)
    transition transform 0.4s

#settingspage.active
    transform translateX(0)



#back-from-settings-button
    background-color: white
    position absolute
    top 10px
    right 10px
    height 40px
    width 40px
    cursor pointer
