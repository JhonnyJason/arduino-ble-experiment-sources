// settingspage styles
#chartpage
    background-color rgba(0, 0, 0, 0.9)
    position fixed
    z-index: 99
    top 0
    lef 0
    height 100vh
    width 100%
    transform translateX(-102%)
    transition transform 0.4s

    h1
        font-size: 20px
        color white

#chartpage.active
    transform translateX(0)


#back-from-chart-button
    background-color: white
    position absolute
    top 10px
    right 10px
    height 30px
    width 30px
    cursor pointer
